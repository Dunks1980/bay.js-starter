<style>
  header {
    display: flex;
    justify-content: center;
    align-items: center;
    padding: 1rem 0;
    box-sizing: border-box;
  }

  .header-container {
    display: flex;
    justify-content: space-between;
    align-items: center;
    width: 100%;
    padding: 0 1rem;
    box-sizing: border-box;
    max-width: "${this.maxwidth || '1100px'}";
  }

  .logo {
    display: flex;
    justify-content: center;
    align-items: center;
    gap: .5rem;
    font-size: 1.3rem;
    font-weight: 700;
    text-decoration: none;
    color: #eee;
    margin-top: -12px;
    margin-left: -12px;
  }

  nav {
    display: flex;
    justify-content: center;
    align-items: center;
    gap: 1.5rem;
  }

  nav a {
    text-decoration: none;
    color: #eee;
    font-size: 1rem;
    font-weight: 700;
    height: 20px;
  }

  .version {
    font-size: 0.75rem;
    align-self: flex-end;
    padding-bottom: 2px;
    color: #888;
  }

  .dunks1980logo {
    transition: all .2s ease-in-out;
  }
  .dunks1980logo:hover {
    transform: scale(1.2)
  }

  .email svg {
    transition: all .2s ease-in-out;
    fill: #ccc;
  }

  .email svg:hover {
    transform: scale(1.2)
  }

  .npm svg {
    transition: all .2s ease-in-out
  }

  .npm svg #Selection1 {
    fill: #ccc
  }

  .npm svg #Selection2 {
    fill: #0000
  }

  .npm svg:hover {
    transform: scale(1.2)
  }

  .npm svg:hover #Selection1 {
    fill: #fff
  }

  .npm svg:hover #Selection2 {
    fill: red
  }

  .github svg {
    transition: all .1s ease-in-out
  }

  .github svg .cls-1 {
    fill: #ccc
  }

  .github svg:hover {
    transform: scale(1.2)
  }

  .github svg:hover .cls-1 {
    fill: #ccc
  }
</style>

